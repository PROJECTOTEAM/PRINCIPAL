<expres><![CDATA[#$pyFunction
def GetLSProData(page_data,Cookie_Jar,m,url = ''):
    from resources.lib.modules import client
    import re,urllib,urlparse

    url = 'http://live.shwidget.com/static/section0.html'

    r = client.request(url)
    r = re.sub('<!--.+?-->', '', r)
    r = re.sub(r'[^\x00-\x7F]+', ' ', r)
    r = client.parseDOM(r, 'div', attrs = {'class': 'lshpanel islive.+?'})

    u = []

    for i in r:
        try:
            c = client.parseDOM(i, 'span', attrs = {'class': 'section'})[0]
            n = '%s | [B]%s[/B]' % (client.parseDOM(i, 'span', attrs = {'class': 'categor.+?'})[0], client.parseDOM(i, 'span', attrs = {'class': 'lshevent'})[0].replace(' Live Stream', ''))
            s = re.split('class\s*=\s*(?:\'|\"|)sectiontableheader', i)
            s = [(client.parseDOM(x, 'td'), client.parseDOM(x, 'tbody')) for x in s]
            s = [(x[0], [y for y in x[1] if not ('"Sawlive"' in y or '"Sportstream365"' in y)]) for x in s]
            s = [(x[0][0].replace(':', ''), ''.join(x[1])) for x in s if x[0]]
            s = [(x[0], re.findall('"(http.+?)"', x[1])) for x in s]
            s = [(x[0], [y for y in x[1] if 'live.shwidget.com' in y]) for x in s]
            s = [(x[0], x[1]) for x in s if x[1]]
            s = [(x[0], [z for y,z in enumerate(x[1]) if z not in x[1][:y]]) for x in s]
            s = [[(x[0], y) for y in x[1]] for x in s]
            s = sum(s, [])

            t = []
            for x1, x2 in s:
                x3 = urlparse.parse_qs(urlparse.urlparse(x2).query)
                try: x2 = x3['event_id'][0]
                except: x2 = '0'
                try: x2 += x3['tv_id'][0]
                except: x2 += '0'
                try: x2 += x3['tid'][0]
                except: x2 += '0'
                try: x2 += x3['channel'][0]
                except: x2 += '0'
                x2 = 'http://live.shwidget.com/static/freeplayer/%s.html' % x2
                t.append((x1, x2))
            if not t: raise Exception()
            t = urllib.quote_plus(str(t))
            u.append((c, n, t))
        except:
            pass

    r = sorted(u, key=lambda x: x[0].lower())
    c = [x[0] for x in r]
    if 'Nfl' in c: c = ['Nfl'] + c
    if 'Hockey' in c: c = ['Hockey'] + c
    if 'Volleyball' in c: c = ['Volleyball'] + c
    if 'Basketball' in c: c = ['Basketball'] + c
    if 'Football' in c: c = ['Football'] + c
    c = [x for y,x in enumerate(c) if x not in c[:y]]

    u = []

    for i in c:
        s = [(x[1], x[2]) for x in r if x[0] == i]
        if s: u += [('[COLOR orange][B]%s[/B][/COLOR]' % i, '')] + s

    return u
]]></expres>
